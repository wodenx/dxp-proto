{
  "name": "dxp-proto-monorepo",
  "description": "",
  "version": "0.0.0",
  "workspaces": [
    "packages/*",
    "sites/*"
  ],
  "keywords": [
    "gatsby"
  ],
  "license": "Apache-2.0",
  "scripts": {
    "build": "turbo build",
    "build:packages": "turbo build --filter=!@sites/--dxp--",
    "fix": "eslint --fix  --cache --ext .js,.jsx,.ts,.tsx packages sites",
    "fix:target": "eslint --fix  --cache --ext .js,.jsx,.ts,.tsx",
    "fix:change": "git diff --name-only --diff-filter=d master... | grep -e 'tsx$' -e 'jsx$' -e 'js$' -e 'ts$' | xargs -L 1 npm run fix:target",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx packages sites --",
    "lint:target": "eslint --cache --ext .js,.jsx,.ts,.tsx",
    "lint:change": "git diff --name-only --diff-filter=d master... | grep -e 'tsx$' -e 'jsx$' -e 'js$' -e 'ts$' | xargs -L 1 npm run lint:target",
    "postinstall": "./postinstall.sh",
    "pw:run": "npx playwright test playwright/tests/smoke/* --project=chromium --workers 1",
    "stackbit": "stackbit dev --runnableDir sites/__dxp__ --port 8000",
    "start": "npm start --workspaces",
    "sync:fix": "syncpack fix-mismatches",
    "sync:check": "syncpack list-mismatches",
    "test": "jest",
    "test:pw": "WAIT_ON_TIMEOUT=600000 start-server-and-test start http://localhost:8000 pw:run",
    "docs": "npm run build:docs --workspace @sites/__dxp__ && docsify serve ./sites/__dxp__/doc"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run sync:check && npm run lint && npm run check"
    }
  },
  "dependencies": {
    "@types/html2canvas": "0.0.35",
    "complexity-report": "^2.0.0-alpha",
    "cypress": "^4.11.0",
    "cypress-failed-log": "^2.6.0",
    "debug": "^4.1.1",
    "html2canvas": "^1.0.0-rc.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "start-server-and-test": "^1.12.0",
    "symlink-dir": "^3.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-transform-modules-commonjs": "7.5.0",
    "@babel/runtime": "^7.4.2",
    "@playwright/test": "1.22",
    "@types/enzyme": "^3.9.1",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.158",
    "@types/rc-tooltip": "^3.7.1",
    "@types/react-dom": "^17.0.2",
    "@types/react": "^17.0.39",
    "@types/slate": "^0.47.9",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "axios": "^0.21.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.9.0",
    "cross-env": "^5.2.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-xpath": "^1.4.0",
    "docsify-cli": "^4.3.0",
    "dotenv": "^8.2.0",
    "enzyme": "^3.9.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^7.27.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.19.0",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "husky": "^3.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-haste-map": "^24.5.0",
    "jest-resolve": "^24.5.0",
    "jest-sonar-reporter": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "ora": "^4.0.2",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^2.6.3",
    "syncpack": "^6.2.0",
    "ts-jest": "^26.4.4",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "turbo": "^1.2.16",
    "typedoc": "^0.22.6",
    "typedoc-plugin-markdown": "^3.11.3",
    "typescript": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dxp-prototype/dxp-proto"
  },
  "engines": {
    "node": ">=16.14"
  },
  "author": ""
}
